[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    class Solution {[m
[32m+[m[32m    protected:[m
[32m+[m[41m    [m	[32mbool IsUglyNumber(int number) {[m
[32m+[m[41m    [m		[32mwhile (number % 5 == 0) {[m
[32m+[m[41m    [m			[32mnumber /= 5;[m
[32m+[m[41m    [m		[32m}[m
[32m+[m[41m    [m		[32mwhile (number % 3 == 0) {[m
[32m+[m[41m    [m			[32mnumber /= 3;[m
[32m+[m[41m    [m		[32m}[m
[32m+[m[41m    [m		[32mwhile (number % 2 == 0) {[m
[32m+[m[41m    [m			[32mnumber /= 2;[m
[32m+[m[41m    [m		[32m}[m
[32m+[m[41m    [m		[32mreturn (number == 1);[m
[32m+[m[41m    [m	[32m}[m
[32m+[m[32m    public:[m
[32m+[m[41m    [m	[32m//遍历每一个数[m
[32m+[m[41m    [m	[32mint GetUglyNumber_Solution(int index) {[m
[32m+[m[41m    [m		[32mif (index <= 0) {[m
[32m+[m[41m    [m			[32mreturn -1;[m
[32m+[m[41m    [m		[32m}[m
[32m+[m[41m    [m		[32mint count = 1;[m
[32m+[m[41m    [m		[32mint num = 0;[m
[32m+[m[41m    [m		[32mwhile (count <= index) {[m
[32m+[m[41m    [m			[32mnum++;[m
[32m+[m[41m    [m			[32mif (IsUglyNumber(num) == 1) {[m
[32m+[m[41m    [m				[32mcout << "count = " << count << ", " << num << " is " << endl;[m
[32m+[m[41m    [m				[32mcount++;[m
[32m+[m[41m    [m			[32m}[m
[32m+[m[41m    [m		[32m}[m
[32m+[m[41m    [m		[32mreturn num;[m
[32m+[m[41m    [m	[32m}[m
[32m+[m[32m    };[m
